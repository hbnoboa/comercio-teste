          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Set_Ano">
      <Query>
        <DataSourceName>DS_Fornecedores</DataSourceName>
        <CommandText>SELECT DISTINCT DIM_Tempo.Ano
FROM            DIM_Tempo INNER JOIN
                         Fato ON DIM_Tempo.IDSK = Fato.IDTempo</CommandText>
      </Query>
      <Fields>
        <Field Name="Ano">
